<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="Pane" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.livechat.controller.ServerController">
   <children>
      <Label layoutX="20.0" layoutY="21.0" prefHeight="32.0" prefWidth="129.0" text="Chat Room">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <ScrollPane fx:id="scrollPane" layoutX="48.0" layoutY="88.0" prefHeight="449.0" prefWidth="304.0">
         <content>
            <VBox fx:id="txtOutPut" prefHeight="448.0" prefWidth="302.0" />
         </content>
      </ScrollPane>
      <ImageView fitHeight="26.0" fitWidth="30.0" layoutX="356.0" layoutY="13.0" onDragDetected="#btnClose" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assest/img/icons8-close-48.png" />
         </image>
      </ImageView>
      <ImageView fx:id="minimize" fitHeight="26.0" fitWidth="26.0" layoutX="318.0" layoutY="13.0" onDragDetected="#btnMinimize" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assest/img/icons8-minimize-window-48%20(1).png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
